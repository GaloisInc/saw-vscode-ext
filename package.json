{
	"name": "saw",
	"displayName": "saw",
	"description": "A SAW (Software Analysis Workbench) plugin for VSCode",
	"version": "0.0.1",
	"publisher": "ajayeeralla",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:saw",
		"onCommand:saw.activate"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ajayeeralla/saw-vscode-ext.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "saw",
				"aliases": [
					"saw",
					"SAW"
				],
				"extensions": [
					".saw"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "saw",
				"scopeName": "source.saw",
				"path": "./syntaxes/saw.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
